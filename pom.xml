            <url>https://gitlab.the-qa-company.com/api/v4/projects/6/packages/maven</url>
        </repository>
    </repositories>
    <distributionManagement>
    <repository>
      <id>gitlab-maven-hdt-endpoint</id>
      <url>https://gitlab.the-qa-company.com/api/v4/projects/1/packages/maven</url>
    </repository>
    <snapshotRepository>
      <id>gitlab-maven-hdt-endpoint</id>
